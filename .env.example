# Copy this file to .env and fill with secure values
# Example: cp .env.example .env && edit .env

# Postgres
POSTGRES_USER=n8n
POSTGRES_PASSWORD=changeme
POSTGRES_DB=n8n

# n8n basic auth (enable or disable)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=strongpassword

# n8n encryption key (must be the same when moving workflows with encrypted credentials)
# Generate a secure 32+ character key
N8N_ENCRYPTION_KEY=replace-with-a-secure-32-char-key

# Optional: set timezone
TZ=Europe/Berlin
